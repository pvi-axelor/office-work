<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  https://axelor.com/xml/ns/object-views/object-views_6.0.xsd">

  <grid model="com.axelor.contact.db.Title" title="Titles" name="title-grid" editable="true">

    <field name="code"/>
    <field name="name"/>

  </grid>

  <form model="com.axelor.contact.db.Title" title="Title" name="title-form">

    <panel title="Title" name="titlePanel">
      <field name="code"/>
      <field name="name"/>
    </panel>

  </form>


</object-views>
