<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  https://axelor.com/xml/ns/object-views/object-views_6.0.xsd">

	<grid model="com.axelor.contact.db.Address" title="Addresses"
		name="address-grid">

		<field name="name" />
		<field name="numberAndStreet" />
		<field name="zip" />
		<field name="city" />
		<field name="country" />

	</grid>

	<form model="com.axelor.contact.db.Address" title="Address"
		name="address-form">

		<panel title="Address">
			<field name="numberAndStreet" />
			<field name="zip" />
			<field name="city" />
			<field name="country" canNew="true" />
		</panel>

	</form>

</object-views>
